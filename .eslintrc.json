{
  "extends": [
    "@zeustech/eslint-config/next",
    "next/core-web-vitals"
  ],
  "rules": {
    "camelcase": "off",
    "react/no-unescaped-entities": "off",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "plugins": [
    "simple-import-sort"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}